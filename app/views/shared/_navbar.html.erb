<div class="navbar-fetchify">
  <div class="container">
    <%= link_to root_path, id: "logo" do %>
      <h2><span class="color-red">F</span>ETCHIF<span class="color-red">Y</span></h2>
    <% end %>
    <ul class="menu-list">
      <li class="active"><h4><%= link_to "NEW DOWNLOAD", "#" %></h4></li>
      <li><h4><%= link_to "MY DOWNLOADS", "#" %></h4></li>
    </ul>
    <div class="dropdown user-avatar">
      <div class="dropdown-toggle" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
        <div>
          <i class="fas fa-user-circle"></i>
          <i class="fas fa-caret-down"></i>
        </div>
      </div>
      <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenuButton">
        <%= link_to "Settings", settings_path, class: "dropdown-item"%>
